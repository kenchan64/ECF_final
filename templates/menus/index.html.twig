{% extends 'base.html.twig' %}


{% block title %}Menus{% endblock %}

{% block body %}
    <div class="text-center mt-5 mb-5">
        <h1>MENUS</h1>
        <div id="main-menu" class="text-center mt-5 mb-5">
            {% for menus in menu %}
                <h3 class="mt-5">{{ menus.title }}</h3>
                <p>{{ menus.setMenu }}</p>
                <p>{{ menus.description }}</p>
                {{ menus.price }} Euros.
            {% endfor %}
        </div>
    </div>
{% endblock %}
